{
  "name": "todo-app",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "nodemon -e js,ejs",
    "start:prod": "node index.js",
    "pretest": "cross-env NODE_ENV=test npx sequelize-cli db:drop && cross-env NODE_ENV=test npx sequelize-cli db:create",
    "test": "cross-env NODE_ENV=test jest --detectOpenHandles",
    "prepare": "cd .. && husky install todo-app/.husky",
    "sequelize": "sequelize",
    "sequelize-cli": "sequelize-cli",
    "migrate": "sequelize db:migrate"
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint",
      "prettier --write"
    ]
  },
  "author": "Nandini",
  "license": "ISC",
  "dependencies": {
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "sequelize": "^6.35.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "mysql2": "^3.6.5",
    "nodemon": "^3.0.2",
    "sequelize-cli": "^6.6.2",
    "supertest": "^6.3.3",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "todo",
    "express",
    "sequelize",
    "jest"
  ],
  "description": "todos project"
}
